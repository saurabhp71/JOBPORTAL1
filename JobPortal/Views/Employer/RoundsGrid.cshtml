<script>

        $(document).ready(function () {
        $(".sweetApprove").click(function () {
            var AppliedJobId = $(this).attr("data-id");
            $.ajax({
                url: "/Employer/UpdateRound1/",
                data: { "AppliedJobId": AppliedJobId },
                success: function (Status) {
                    swal({
                        title: "Your Candidate is Approved..",
                        button: "ok",
                        // dangerMode: true,

                    })
                        //.then(function () {
                        //    window.location.href = "/Employer/ApprovalEmail?AppliedJobId=" + AppliedJobId;
                        //})
                        .then(function () {
                            window.location.href = "/Employer/RoundsGrid";
                        })
                }
            });
        });
        $(".sweetReject").click(function () {
            var AppliedJobId = $(this).attr("data-id");
            $.ajax({
                url: "/Employer/RejectatRound1/",
                data: { "AppliedJobId": AppliedJobId },
                success: function (Status) {
                    swal({
                        title: "Your Candidate is Rejected..",
                        button: "ok",
                        // dangerMode: true,

                    })
                        .then(function () {
                            window.location.href = "/Employer/RejectionEmail?AppliedJobId=" + AppliedJobId;
                        })
                        .then(function () {
                            window.location.href = "/Employer/RoundsGrid";
                        })

                }
            });
        });
        /* $(document).ready(function () {*/
        var Applylist = new Array();
        $('#btnApprove').click(function () {
            var selectedApplylist = new Array();
            var val = [];
            $("#multiselect:checked").each(function () {
                selectedApplylist.push($(this).val());

            });
            var Applylist = selectedApplylist.toString();
            $.get("/Employer/UpdateRound1list", {
                Applylist: Applylist
            })
                .then(function () {
                    window.location.href = "/Employer/RoundsGrid";
                })
        });

        $('#btnReject').click(function () {
            var selectedApplylist = new Array();
            var val = [];
            $("#multiselect:checked").each(function () {
                selectedApplylist.push($(this).val());

            });
            var Applylist = selectedApplylist.toString();
            $.get("/Employer/RejectatRound1list", {
                Applylist: Applylist
            })
                .then(function () {
                    window.location.href = "/Employer/RoundsGrid";
                })
        });

        $('#btnOpen').click(function () {
            var selectedApplylist = new Array();
            var val = [];
            var email = [];
            $("#multiselect:checked").each(function () {
                selectedApplylist.push($(this).val());

            });
            var email = document.getElementsByClassName("email");
            var email1 = [];
            for (var i = 0; i < selectedApplylist.length; i++) {
                email1.push(email[i].outerText);
            }

            sessionStorage.setItem("selected", selectedApplylist);
            sessionStorage.setItem("email", email1)
            var Email = email1.toString();
            var Applylist = selectedApplylist.toString();
            //$.post("/Employer/CreateRoundwithDetails",function (Applylist) {
            //    Applylist: Applylist


            $.ajax({
                contentType: 'application/json; charset=utf-8',
                /*dataType: 'json',*/
                type: 'GET',
                url: '/Employer/CreateRoundwithDetails',
                data: { Applylist: Applylist, Email: Email },
                /*data: { Email: Email },*/
                success: function (data) {
                    $("#partialModal").find(".modal-body").html(data);
                    $("#partialModal").modal('show');
                },
                failure: function (response) {
                    alert("method failed")
                }
            });

        });
        $("#selectall").click(function () {
            if (this.checked) {
                $('.checkbox').each(function () {
                    $(".checkbox").prop('checked', true);
                })
            } else {
                $('.checkbox').each(function () {
                    $(".checkbox").prop('checked', false);
                })
            }
        });

        $('.checkbox').click(function () {
            if ($('.checkbox').is(':checked')) {
                $('#btnApprove').show();
                $('#btnReject').show();
            }
            ///* else if ($('#Disclosed').is(':checked')) {*/
            // //    $('#flexible').hide();
            // //    $('#fixed').hide();
            // }
            else {
                $('#btnApprove').hide();
                $('#btnReject').hide();
            }
        });
        $('#selectall').click(function () {
            if ($('#multiselect').is(':checked')) {
                $('#btnApprove').show();
                $('#btnReject').show();
            }
            ///* else if ($('#Disclosed').is(':checked')) {*/
            // //    $('#flexible').hide();
            // //    $('#fixed').hide();
            // }
            else {
                $('#btnApprove').hide();
                $('#btnReject').hide();
            }
        });
    });

</script>
